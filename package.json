{
  "name": "knex",
  "version": "1.0.0",
  "description": "Query Builder Knex",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "knex:migrate": "knex --knexfile knexfile.ts migrate:latest",
    "knex:seed": "knex --knexfile knexfile.ts seed:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gease-RS/Node-Knex.git"
  },
  "keywords": [
    "node",
    "knex",
    "postgresql",
    "query",
    "builder"
  ],
  "author": "Gease Oliveira da  Rosa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Gease-RS/Node-Knex/issues"
  },
  "homepage": "https://github.com/Gease-RS/Node-Knex#readme",
  "dependencies": {
    "express": "^4.17.1",
    "knex": "^0.21.15",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
